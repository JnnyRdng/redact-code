{
  "name": "redact-code",
  "displayName": "redact-code",
  "description": "Visually Redact Strings",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "redact-code.redactedWords" : {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "List of strings to redact."
        }
      }
    },
    "commands": [
      {
        "command": "redact-code.addRedactedWord",
        "title": "Add a word to redact"
      },
      {
        "command": "redact-code.activate",
        "title": "Redact code"
      },
      { 
        "command": "redact-code.deactivate",
        "title": "Reveal redacted code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
